    </changeSet>

    <changeSet id="v060-5" author="ikoriachenko">
        <loadUpdateData file="csv/mappingUL.csv"
                        primaryKey="condition_id,tag_type_id"
                        relativeToChangelogFile="true"
                        tableName="notification_juridical_condition_tag_type">
        </loadUpdateData>
    </changeSet>

</databaseChangeLog>
