    </changeSet>

    <changeSet id="v060-4" author="ikoriachenko">
        <loadUpdateData file="csv/conditionUL.csv"
                        primaryKey="id"
                        relativeToChangelogFile="true"
                        tableName="notification_juridical_tag_condition">
